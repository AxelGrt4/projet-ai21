{
    "java.configuration.updateBuildConfiguration": "interactive",
    "[java]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "redhat.java"
    },
    "ltex.language": "en",
    "cSpell.language": "en,fr",
    "cSpell.words": [
        "antlr",
        "choco",
        "lelelele",
        "mvnw",
        "ojdkbuild",
        "setx",
        "stackoverflow",
        "takari",
        "winget"
    ],
    "files.exclude": {
        "src/main/java/fr/utc/parsing": false,
        "src/main/resources/grammar/.antlr/": true,
        "target/": true
    },
    "redhat.telemetry.enabled": false,
}
